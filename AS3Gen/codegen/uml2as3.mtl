[comment encoding = UTF-8 /]
[module uml2as3('http://www.eclipse.org/uml2/4.0.0/UML')]

[template public generate(p : Package)]
[comment @main/]
[file (p.name.concat('.txt'), false, 'UTF-8')]

[p.genClassif()/]

[/file]
[/template]

[template public genClassif(c : Class)]
[file (c.name.concat('.as'), false, 'UTF-8')]
package
{
	[c.visibility/] class [c.name/] {

	}
}
[/file]
[/template] 





